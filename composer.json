{
    "name": "scaletta/botman-driver-dialogflow",
    "license": "MIT",
    "description": "Dialogflow fulfillment driver for BotMan with Fixes",
    "keywords": [
        "Bot",
        "BotMan",
        "Dialogflow",
        "Fixes"
    ],
    "homepage": "http://github.com/scaletta/botman-driver-dialogflow",
    "authors": [
        {
            "name": "G.T Nomikos",
            "email": "g.t.nomikos@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "botman/botman": "~2.1",
        "eristemena/dialogflow-fulfillment-webhook-php": "dev-master"
    },
    "require-dev": {
        "botman/studio-addons": "~1.0",
        "phpunit/phpunit": "~5.0",
        "illuminate/console": "~5.0",
        "illuminate/support": "~5.0",
        "mockery/mockery": "dev-master",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "BotMan\\Drivers\\Dialogflow\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BotMan\\Drivers\\Dialogflow\\Providers\\DialogflowServiceProvider"
            ]
        }
    }
}
